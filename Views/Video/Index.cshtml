@model IEnumerable<WarriorCombatAcademyTrainer.Models.Video>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RankID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VideoTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VideoSrc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsVideoComplete)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VideoRank)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RankID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VideoTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VideoSrc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsVideoComplete)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VideoRank)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
